		<td><%= check_box "cn_ex_eq", cn_ex[1]["visite_protocolaire"].id.to_s+"]["+cn_ex[1]["idequipement"].to_s,{:checked =>!sel.empty? },checked_value ="yes"+val_check,unchecked_value="no"+val_check %> </td>
