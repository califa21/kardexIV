<%if (!cn_ex[1]["visite_protocolaire"].est_annule) &&((cn_ex[1]["visite_protocolaire"].type_potentiel.type_potentiel!="Une fois") || ( cn_ex[1]["date_visite"]=="pas applique "))   then %>
